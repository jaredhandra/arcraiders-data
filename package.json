{
  "name": "@arcraiders/data",
  "version": "1.0.1",
  "description": "Arc Raiders game data and types",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types",
    "items",
    "scripts"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:types": "node ./scripts/generate-types.js",
    "build": "tsc",
    "prepublishOnly": "pnpm run generate:types && pnpm run build"
  },
  "keywords": [
    "arc-raiders",
    "game-data",
    "types"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
